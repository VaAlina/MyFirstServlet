http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<servlet>
	<servlet-name>Forms</servlet-name>
	<servlet-class>ua.kiev.prog.FormsServlet</servlet-class>
</servlet>
<servlet-mapping>
